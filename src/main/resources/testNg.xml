<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<!-- suite节点：测试套件，
       test节点：一个测试场景
        classes节点：一个测试用例
           methods节点：一个测试方法
             include节点：包括
             exclude节点：不包括
  -->

<suite name="AllTests">
    <test verbose="2" preserve-order="true" name="/Users/liucong/Downloads/javaprojects/webauto_java">
        <classes>
            <class name="com.curry.webauto_java.scripts.TestDemo">
                <methods>
                    <include name="test_login"/>
                    <include name="test_shop"/>
                    <include name="test_order"/>
                    <include name="test_logout"/>
                    <include name="testDataProvider"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>